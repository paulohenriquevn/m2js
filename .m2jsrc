{
  "deadCode": {
    "enableCache": true,
    "maxCacheSize": 1000,
    "chunkSize": 50,
    "showProgress": false,
    "format": "table",
    "includeMetrics": true,
    "includeSuggestions": true
  },
  "extraction": {
    "includeComments": true,
    "includeUsageExamples": false,
    "includeBusinessContext": false,
    "includeArchitectureInsights": false,
    "includeSemanticAnalysis": false
  },
  "files": {
    "extensions": [
      ".ts",
      ".tsx",
      ".js",
      ".jsx"
    ],
    "ignorePatterns": [
      "**/node_modules/**",
      "**/dist/**",
      "**/build/**",
      "**/.next/**",
      "**/coverage/**",
      "**/*.test.*",
      "**/*.spec.*"
    ],
    "maxFileSize": 10
  },
  "output": {
    "format": "markdown",
    "preserveStructure": true
  }
}